{
  "name": "finalproject",
  "version": "0.1.0",
  "homepage": "./",
  "private": true,
  "main": "main.js",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "ajv": "^8.17.1",
    "ajv-keywords": "^5.1.0",
    "axios": "^1.11.0",
    "better-sqlite3": "^12.2.0",
    "bl": "^6.1.3",
    "chatgpt": "^5.2.5",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "framer-motion": "^12.23.12",
    "ip-address": "^10.0.1",
    "jszip": "^3.10.1",
    "lottie-react": "^2.4.1",
    "node-fetch": "^3.3.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router": "^7.8.0",
    "react-router-dom": "^7.8.2",
    "react-scripts": "5.0.1",
    "react-scroll-to-top": "^3.1.0",
    "react-syntax-highlighter": "^15.6.6",
    "react-use-cart": "^1.14.0",
    "reactstrap": "^9.2.3",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "electron": "^38.1.0",
    "electron-builder": "^24.13.3",
    "wait-on": "^7.0.1"
  },
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "electron-start": "electron .",
    "dev": "concurrently \"npm run react-start\" \"wait-on http://localhost:3000 && electron .\"",
    "build": "npm run react-build",
    "dist": "npm run build && electron-builder --win --x64 --publish never",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "build": {
    "appId": "com.ahmed.codesnip",
    "productName": "CodeSnip",
    "extends": null,
    "files": [
      "build/**/*",
      "main.js",
      "preload.js",
      "db.js",
      "package.json"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "forceCodeSigning": false
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}